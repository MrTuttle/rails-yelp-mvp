<h1>New</h1>
<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.submit %>
  <%= redirect_to restaurant.name, restaurant_path(restaurant) %>
<% end %>
<%= link_to "See all restaurants", restaurants_path %>
